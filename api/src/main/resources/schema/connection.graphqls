type Connection {
    id: ID!
    participantFrom: Participant
    participantTo: Participant
    verificationStage: VerificationStage!
    services: [ConnectionService!]!
}

enum VerificationStage {
    PENDING
    PROGRESS
    VERIFIED
    REJECTED
}

type ConnectionList implements PaginatedList {
    items: [Connection]
    totalItems: Int!
}
