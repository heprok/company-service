type Service {
    id: ID!
    companyId: ID!
    name: String!
    price: Float
    image: Image
    verifiedUses: Int
    lastUsed: Date
}

type ServiceList implements PaginatedList {
    items: [Service!]!
    totalItems: Int!
}

input ServiceFilter {
    numberUses: [Int],
    cost: [Float],
    lastUsed: [Date]
}
