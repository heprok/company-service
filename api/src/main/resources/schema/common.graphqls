scalar Date
scalar Upload
scalar Url
scalar Year

directive @skipcodegen on FIELD_DEFINITION

type Image {
    url: Url
}

type Error {
    code: String!
    field: [String!]
}

input RangeInt {
    start: Int
    end: Int
}

input RangeDate {
    start: Date
    end: Date
}

input RangeFloat {
    start: Float
    end: Float
}

type BaseResult {
    success: Boolean
}

interface ResultWithUserErrors {
    userErrors: [Error]!
}

interface PaginatedList {
    totalItems: Int!
}
